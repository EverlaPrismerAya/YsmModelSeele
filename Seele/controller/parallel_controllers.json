{
	"format_version": "1.19.0",
	"animation_controllers": {
		"player.pre_parallel_0": {
			"states": {
				"default": {
					"animations": ["pre_parallel0", "pre_parallel1", "pre_parallel2", "pre_parallel3", "pre_parallel4", "pre_parallel5", "pre_parallel6", "pre_parallel7"]
				}
			}
		},
		"player.pre_parallel_1": {
			"initial_state": "idle",
			"states": {
				"idle": {
					"animations": ["idle"],
					"sound_effects": [
						{
							"effect": ""
						}
					],
					"transitions": [
						{"walk": "ctrl.walk && ysm.input_vertical >=0 &&query.is_on_ground"},
						{"walk_swap": "ctrl.walk && ysm.input_vertical <0 &&query.is_on_ground"},
						{"jump": "ctrl.jump && !query.is_on_ground"},
						{"fly": "ctrl.fly && !query.is_on_ground"},
						{"sneak": "ctrl.sneak"},
						{"sneaking": "ctrl.sneaking"},
						{"elytra_fly": "ctrl.elytra_fly"},
						{"climb": "ctrl.climb || ctrl.climbing"},
						{"climbing": "ctrl.climb || ctrl.climbing"}
					],
					"blend_transition": 0.3,
					"blend_via_shortest_path": true
				},
				"walk": {
					"animations": ["walk_normal"],
					"transitions": [
						{"idle": "ctrl.idle"},
						{"run": "ctrl.run"},
						{"walk_swap": "ctrl.walk && ysm.input_vertical <0"},
						{"jump": "ctrl.jump"},
						{"fly": "ctrl.fly"},
						{"sneak": "ctrl.sneak"},
						{"elytra_fly": "ctrl.elytra_fly"},
						{"climb": "ctrl.climb || ctrl.climbing"},
						{"climbing": "ctrl.climb || ctrl.climbing"}
					],
					"blend_transition": 0.2
				},
				"run": {
					"animations": ["run"],
					"transitions": [
						{"idle": "ctrl.idle"},
						{"walk": "ctrl.walk"},
						{"walk_swap": "ysm.input_vertical <0"},
						{"jump": "ctrl.jump"},
						{"fly": "ctrl.climb"},
						{"sneak": "ctrl.sneak"},
						{"climb": "ctrl.climb || ctrl.climbing"},
						{"climbing": "ctrl.climb || ctrl.climbing"}
					],
					"blend_transition": 0.2
				},
				"walk_swap": {
					"animations": ["walk_swap"],
					"transitions": [
						{"idle": "ctrl.idle"},
						{"walk": "ctrl.walk && ysm.input_vertical >=0"},
						{"run": "ysm.input_vertical >1"},
						{"jump": "ctrl.jump"},
						{"fly": "ctrl.fly"},
						{"sneak_swap": "ysm.input_vertical <0 &&ctrl.sneak"},
						{"climb": "ctrl.climb || ctrl.climbing"},
						{"climbing": "ctrl.climb || ctrl.climbing"},
						{"sneak": "ysm.input_vertical >=0 &&ctrl.sneak"}
					],
					"blend_transition": 0.2,
					"blend_via_shortest_path": true
				},
				"jump": {
					"animations": ["jump_normal"],
					"transitions": [
						{"idle": "!ctrl.jump"},
						{"walk": "!ctrl.jump"},
						{"run": "!ctrl.jump"},
						{"walk_swap": "!ctrl.jump"},
						{"fly": "ctrl.fly && !query.is_on_ground"},
						{"sneak": "!ctrl.jump && ctrl.sneak"},
						{"sneaking": "!ctrl.jump && ctrl.sneaking"},
						{"sneak_swap": "!ctrl.jump && ctrl.sneak"},
						{"climb": "ctrl.climb || ctrl.climbing"},
						{"climbing": "ctrl.climb || ctrl.climbing"}
					],
					"blend_transition": 0.2
				},
				"sneaking": {
					"animations": ["sneaking"],
					"transitions": [
						{"fly": "ctrl.fly"},
						{"idle": "!ctrl.sneaking"},
						{"sneak": "ctrl.sneak"},
						{"jump": "ctrl.jump"},
						{"climb": "ctrl.climb || ctrl.climbing"},
						{"climbing": "ctrl.climb || ctrl.climbing"},
						{"walk": "ctrl.walk"}
					],
					"blend_transition": 0.2
				},
				"sneak": {
					"animations": ["sneak_normal"],
					"transitions": [
						{"fly": "ctrl.fly"},
						{"idle": "!ctrl.sneak"},
						{"sneaking": "ctrl.sneaking"},
						{"sneak_swap": "ysm.input_vertical <0 &&ctrl.sneak"},
						{"jump": "ctrl.jump"},
						{"climb": "ctrl.climb || ctrl.climbing"},
						{"climbing": "ctrl.climb || ctrl.climbing"},
						{"walk": "ysm.input_vertical >=0 &&!ctrl.sneak &&!ctrl.sneaking"}
					],
					"blend_transition": 0.2
				},
				"sneak_swap": {
					"animations": ["sneak_swap"],
					"transitions": [
						{"sneak": "ysm.input_vertical >=0 &&ctrl.sneak"},
						{"sneaking": "!ctrl.sneak && ctrl.sneaking"},
						{"idle": "!ctrl.sneak && ctrl.idle"},
						{"jump": "ctrl.jump"},
						{"walk_swap": "ysm.input_vertical <0 &&!ctrl.sneak &&!ctrl.sneaking"},
						{"climb": "ctrl.climb || ctrl.climbing"},
						{"climbing": "ctrl.climb || ctrl.climbing"}
					],
					"blend_transition": 0.2
				},
				"fly": {
					"animations": ["fly_normal"],
					"on_entry": ["ctrl.fly;"],
					"transitions": [
						{"idle": "!ctrl.fly && ctrl.idle"},
						{"walk": "!ctrl.fly && ctrl.walk"},
						{"run": "!ctrl.fly &&  ctrl.run"},
						{"jump": "!ctrl.fly &&  ctrl.jump"},
						{"sneaking": "!ctrl.fly && ctrl.sneaking"},
						{"sneak": "!ctrl.fly &&  ctrl.sneak"},
						{"elytra_fly": "!ctrl.fly && ctrl.elytra_fly\t"},
						{"climb": "ctrl.climb || ctrl.climbing"},
						{"climbing": "ctrl.climb || ctrl.climbing"}
					],
					"blend_transition": 0.2
				},
				"elytra_fly": {
					"animations": ["elytra_fly"],
					"transitions": [
						{"idle": "!ctrl.elytra_fly && ctrl.idle\t"},
						{"walk": "!ctrl.elytra_fly && ctrl.walk"},
						{"jump": "!ctrl.elytra_fly && ctrl.jump"},
						{"fly": "!ctrl.elytra_fly && ctrl.fly"},
						{"climb": "ctrl.climb || ctrl.climbing"},
						{"climbing": "ctrl.climb || ctrl.climbing"}
					],
					"blend_transition": 0.3
				},
				"climb": {
					"animations": ["climb"],
					"transitions": [
						{"idle": "!ctrl.climb && !ctrl.climbing"},
						{"climbing": "ctrl.climbing"}
					],
					"blend_transition": 0.2
				},
				"climbing": {
					"animations": ["climbing"],
					"transitions": [
						{"idle": "!ctrl.climb && !ctrl.climbing"},
						{"climb": "ctrl.climb"}
					],
					"blend_transition": 0.2
				}
			}
		},
		"player.parallel_0": {
			"states": {
				"default": {
					"animations": ["parallel0", "parallel1", "parallel2", "parallel3", "parallel4", "parallel5", "parallel6", "parallel7"]
				}
			}
		},
		"player.parallel_1": {
			"states": {
				"default": {
					"animations": ["attack_empty"],
					"on_exit": ["v.swing_sword=0;v.attack=math.mod(v.attack+1,3);"],
					"transitions": [
						{"attack1": "v.swing_sword&&v.attack==0&&!v.jump"},
						{"attack2": "v.swing_sword&&v.attack==1&&!v.jump"},
						{"attack3": "v.swing_sword&&v.attack==2&&!v.jump"},
						{"attack_jump": "v.swing_sword&&v.jump"}
					]
				},
				"attack1": {
					"animations": [
						{"sword_attack_01": "!ctrl.idle"},
						{"sword_idle_attack_01": "ctrl.idle"}
					],
					"on_exit": ["v.swing_sword=0;v.attack=math.mod(v.attack+1,3);"],
					"transitions": [
						{"attack2": "v.swing_sword&&v.attack==1&&!v.jump"},
						{"default": "q.all_animations_finished"},
						{"attack_jump": "v.swing_sword&&v.jump"}
					],
					"blend_transition": 0.05,
					"blend_via_shortest_path": true
				},
				"attack2": {
					"animations": [
						{"sword_attack_02": "!ctrl.idle"},
						{"sword_idle_attack_02": "ctrl.idle"}
					],
					"on_exit": ["v.swing_sword=0;v.attack=math.mod(v.attack+1,3);"],
					"transitions": [
						{"attack3": "v.swing_sword&&v.attack==2&&!v.jump"},
						{"default": "q.all_animations_finished"},
						{"attack_jump": "v.swing_sword&&v.jump"}
					],
					"blend_transition": 0.05,
					"blend_via_shortest_path": true
				},
				"attack3": {
					"animations": [
						{"sword_attack_03": "!ctrl.idle"},
						{"sword_idle_attack_03": "ctrl.idle"}
					],
					"on_exit": ["v.swing_sword=0;v.attack=math.mod(v.attack+1,3);"],
					"transitions": [
						{"default": "q.all_animations_finished"},
						{"attack1": "v.swing_sword&&v.attack==0&&!v.jump"},
						{"attack_jump": "v.swing_sword&&v.jump"}
					],
					"blend_transition": {
						"0.0": 1,
						"0.0056": 0.96985,
						"0.0111": 0.88302,
						"0.0167": 0.75,
						"0.0222": 0.58682,
						"0.0278": 0.41318,
						"0.0333": 0.25,
						"0.0389": 0.11698,
						"0.0445": 0.03015,
						"0.05": 0
					},
					"blend_via_shortest_path": true
				},
				"attack_jump": {
					"animations": ["sword_jump_attack"],
					"on_exit": ["v.swing_sword=0;v.attack=math.mod(v.attack+1,3);"],
					"transitions": [
						{"default": "q.all_animations_finished"},
						{"attack1": "v.swing_sword&&!v.jump"}
					],
					"blend_transition": 0.05,
					"blend_via_shortest_path": true
				}
			}
		}
	}
}